{
    "name": "citadel-websocket-client",
    "version": "1.0.0",
    "description": "TypeScript WebSocket client for Citadel Internal Service",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "test": "npm run build && node dist/test.js",
        "generate-types": "cd ../../.. && cargo run --features typescript --example generate_simple_types",
        "dev": "npm run generate-types && npm run build && npm run test",
        "setup": "npm install && npm run generate-types"
    },
    "devDependencies": {
        "@types/node": "^20.10.0",
        "@types/uuid": "^10.0.0",
        "@types/ws": "^8.5.10",
        "typescript": "^5.3.0"
    },
    "dependencies": {
        "uuid": "^9.0.1",
        "ws": "^8.14.2"
    }
}